{
  "name": "serverless_crud_api",
  "version": "1.0.0",
  "description": "note crud api using serverless framework + ts + middy middleware +jest",
  "main": "serverless.yml",
  "scripts": {
    "test-local": "jest",
    "lint": "eslint --ext .ts --fix .",
    "prettier": "prettier --write --ignore-unknown .",
    "prepare": "husky install",
    "build": "tsc --noemit",
    "deploy": "serverless deploy",
    "start-db": "serverless dynamodb start",
    "offline": "serverless dynamodb start && serverless offline"
  },
  "devDependencies": {
    "@aws-lambda-powertools/logger": "^1.8.0",
    "@aws-sdk/client-dynamodb": "^3.494.0",
    "@aws-sdk/lib-dynamodb": "^3.494.0",
    "@middy/core": "^4.3.1",
    "@middy/http-cors": "^4.5.5",
    "@middy/http-error-handler": "^4.3.1",
    "@middy/http-json-body-parser": "^4.3.1",
    "@middy/http-security-headers": "^4.3.1",
    "@middy/input-output-logger": "^4.3.1",
    "@types/aws-lambda": "^8.10.119",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "dynamodb-toolbox": "^0.9.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "http-status-codes": "^2.2.0",
    "husky": "^8.0.0",
    "prettier": "^3.2.4",
    "serverless": "^3.34.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "serverless-add-api-key": "^4.2.1",
    "serverless-esbuild": "^1.46.0",
    "serverless-offline": "^12.0.4",
    "serverless-dynamodb": "^0.2.47",
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "middy-zod-validator": "^3.0.4",
    "zod": "^3.21.4"
  },
  "author": "snarjun98",
  "license": "MIT",
  "dependencies": {
    "lint-staged": "^15.2.0"
  }
